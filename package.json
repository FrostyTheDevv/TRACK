{
  "name": "track",
  "version": "1.0.0",
  "description": "Discord bot for tracking live streams across multiple platforms with web dashboard",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "dev:dashboard": "cd dashboard/static && python -m http.server 8000",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test:scrapers": "npx ts-node test/test-scrapers.ts",
    "test:database": "npx ts-node test/test-database.ts",
    "test": "npm run test:scrapers && npm run test:database"
  },
  "keywords": [
    "discord",
    "bot",
    "stream",
    "tracker",
    "twitch",
    "youtube",
    "tiktok",
    "kick"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "discord.js": "^14.21.0",
    "dotenv": "^17.1.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "mongoose": "^8.16.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.0",
    "nodemon": "^3.1.10",
    "playwright": "^1.53.2",
    "puppeteer": "^24.12.0",
    "rimraf": "^6.0.1",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "typescript": "^5.8.3",
    "user-agents": "^1.1.594",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.12",
    "concurrently": "^9.2.0",
    "ts-node": "^10.9.2"
  }
}
